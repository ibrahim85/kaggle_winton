    "    for i in range(nFeatures):\n",
    "        nidx_i = np.isnan(datan[:,i])\n",
    "        kde_i = stats.gaussian_kde(datan[~nidx_i,i])\n",
    "        kdes.append(kde_i)\n",
    "    \n",
    "    # draw randomly from the sample distribution to fill in the missing values\n",
